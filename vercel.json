{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTION"
            ],
            "dest": "/index.js"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}