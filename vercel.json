{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/transactions/(.*)",
            "dest": "/index.js"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}